<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PluginTools" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Win32">
				<Option output="..\..\..\Debug\PluginTools" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\..\Debug\OBJ\PluginTools" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_CONSOLE" />
					<Add option="-W" />
					<Add option="-g" />
					<Add option="-O0" />
					<Add directory="$(BOOST_ROOT)" />
					<Add directory="$(SolutionDir)\..\src\Foundation\" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(BOOST_ROOT)" />
					<Add directory="$(SolutionDir)\..\src\Foundation\" />
				</ResourceCompiler>
				<Linker>
					<Add library="BOE.PluginManager" />
					<Add directory="..\..\..\Debug" />
				</Linker>
			</Target>
			<Target title="Release Win32">
				<Option output="Release\PluginTools" prefix_auto="1" extension_auto="1" />
				<Option object_output="Release" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DWIN32" />
					<Add option="-DNDEBUG" />
					<Add option="-D_CONSOLE" />
					<Add option="-W" />
				</Compiler>
			</Target>
		</Build>
		<Unit filename="ReadMe.txt" />
		<Unit filename="PluginTools.cpp" />
		<Unit filename="stdafx.cpp" />
		<Unit filename="stdafx.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
